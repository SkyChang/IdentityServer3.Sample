
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Detail</title>
</head>
<body>
    <div> 

        @model IEnumerable<System.Security.Claims.Claim>

        <a href="~/Auth/Logout">Logout</a>

        <dl>
            @foreach (var claim in Model)
            {
                <dt>@claim.Type</dt>
                <dd>@claim.Value</dd>
            }
        </dl>

    </div>
</body>
</html>
